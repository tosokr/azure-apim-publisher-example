{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ApimServiceName": {
            "type": "string",
            "metadata": {
                "description": "The name of the API Management"
            }
        }
    },
    "variables": {},
    "resources": [
        {
           "type": "Microsoft.ApiManagement/service/products",
            "apiVersion": "2019-01-01",
            "name": "[concat(parameters('ApimServiceName'), '/testproduct')]",            
            "properties": {
                "displayName": "testproduct",
                "description": "This is TEST product",
                "subscriptionRequired": true,
                "approvalRequired": false,
                "state": "notPublished"
            }
        },
        {
            "type": "Microsoft.ApiManagement/service/products",
             "apiVersion": "2019-01-01",
             "name": "[concat(parameters('ApimServiceName'), '/todoproduct')]",            
             "properties": {
                 "displayName": "todoproduct",
                 "description": "This is TODO product",
                 "subscriptionRequired": true,
                 "approvalRequired": false,
                 "state": "published"
             }
         }       
    ],
    "outputs": {}
}
